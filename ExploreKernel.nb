Cell[BoxData[{
 RowBox[{
  RowBox[{"directions", "=", 
   RowBox[{"Normalize", "/@", "perips"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"pairangles", "=", 
   RowBox[{"Flatten", "@", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"ArcCos", "[", 
       RowBox[{
        RowBox[{"directions", "[", 
         RowBox[{"[", "i", "]"}], "]"}], ".", 
        RowBox[{"directions", "[", 
         RowBox[{"[", "j", "]"}], "]"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", 
        RowBox[{
         RowBox[{"Length", "@", "directions"}], "-", "1"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", 
        RowBox[{"i", "+", "1"}], ",", 
        RowBox[{"Length", "@", "directions"}]}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Kerneldim", "=", 
   RowBox[{"Last", "@", 
    RowBox[{"Dimensions", "[", "directions", "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"simplexangle", "=", 
   RowBox[{"N", "@", 
    RowBox[{"ArcCos", "[", 
     RowBox[{
      RowBox[{"-", "1"}], "/", "Kerneldim"}], "]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"simplexline", "=", 
   RowBox[{"ListLinePlot", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
