       RowBox[{"simplexangle", ",", 
        RowBox[{"Max", "@", 
         RowBox[{"BinCounts", "[", 
          RowBox[{"pairangles", ",", 
           RowBox[{"Pi", "/", 
            RowBox[{"Floor", "[", 
             RowBox[{"1", "+", 
              RowBox[{"Log2", "[", 
               RowBox[{"Length", "@", "pairangles"}], "]"}]}], "]"}]}]}], 
          "]"}]}]}], "}"}]}], "}"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Show", "[", 
  RowBox[{
   RowBox[{"Histogram", "[", "pairangles", "]"}], ",", "simplexline"}], 
